  <!-- popup definition -->
  <layout id="RTRN_popup" filename="PhonePlus-Alpha.svg">
    <box compact="true">
      <key id="settings"/>
      <key id="move"/>
      <key id="hide" svg_id="hide.popup" image="close.svg" group="nowordlist"/>
    </box>
  </layout>
