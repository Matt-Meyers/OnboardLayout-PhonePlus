<keyboard id="PhonePlus" format="3.2" section="system" summary="Mobile keyboard with number row">
  <include file="key_defs.xml"/>
  <!-- box containing all the keys and buttons -->
  <!-- the rows of keys on the Onboard keyboard are counted bottom to top starting with 1 -->
  <box border="0.5" spacing="2.0" orientation="vertical">
    <!--- word suggestions -->
    <panel filename="PhonePlus-Alpha.svg">
      <include file="word_suggestions.xml"/></panel>
    <key_template button="true" id="layer0" show_active="false"/>
    <panel>
      <box orientation="vertical">
        <!-- alpha and numbers layers -->
        <panel>
          <!-- Cyrillic keymaps-->
          <panel id="alpha-keys" filename="PhonePlus-Alpha.svg" layer="alpha" layout="ru,ua,by,bg,tj,ge(os),az(cyrillic),mn,mk">
            <!-- row 2,3 and 4 of the layers -->
            <!-- row 5 new high row -->
            <key group="alphanumeric" id="AE01" keysym="0x31" label="1"/>
            <key group="alphanumeric" id="AE02" keysym="0x32" label="2"/>
            <key group="alphanumeric" id="AE03" keysym="0x33" label="3"/>
